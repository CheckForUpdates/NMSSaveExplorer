{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Main",
      "request": "launch",
      "mainClass": "com.nmssaveexplorer.Main",
      "projectName": "nms-ship-seed-editor-NMSSaveExplorer"
    },
    {
      "type": "java",
      "name": "Launch Main (Gradle)",
      "request": "launch",
      "mainClass": "com.nmssaveexplorer.SaveManagerApp",
      "projectName": "nms-ship-seed-editor",
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "gradle build",
      "vmArgs": "--add-modules javafx.controls,javafx.fxml"
    },
    {
      "type": "java",
      "name": "Debug (Attach to Gradle App)",
      "request": "attach",
      "hostName": "localhost",
      "port": 5005
    }
  ]
}
